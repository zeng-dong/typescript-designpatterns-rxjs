	"devDependencies": {
		"@babel/preset-env": "^7.18.10",
		"@tsconfig/recommended": "^1.0.1",
		"eslint": "7.30.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-import": "2.23.4",
